{
  "name": "LaTeX Dev",
  "image": "texlive/texlive:latest-full",
  "settings": {
    "latex-workshop.latex.outDir": "./out"
  },
  "extensions": [
    "james-yu.latex-workshop"
  ],
  "workspaceFolder": "/workdir",
  "mounts": [
    "source=${localWorkspaceFolder},target=/workdir,type=bind",
    "source=${localWorkspaceFolder}/../mdocean/simulation/modules/OpenFLASH/pubs/src,target=/workdir/../mdocean/simulation/modules/OpenFLASH/pubs/src,type=bind"
  ],
  "remoteUser": "root"
}
